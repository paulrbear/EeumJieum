<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dev.erica.hyunji.eeumjieum.DetailWorkReportViewActivity">

    //top title bar
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:background="@color/colorCyanMain">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/topBar_Height"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="55dp">
                <Button
                    android:id="@+id/backbtn"
                    android:layout_marginTop="@dimen/topBar_Icon_Margin_Top"
                    android:layout_marginLeft="@dimen/topBar_Icon_Margin"
                    android:layout_width="@dimen/topBar_Icon_Width"
                    android:layout_height = "@dimen/topBar_Icon_Height"
                    android:background="@drawable/back_w_btn"
                    android:onClick="onClick_backbtn"/>
            </LinearLayout>
            <LinearLayout
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="55dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:layout_marginTop="15.2dp"
                    android:text="근무일지"
                    android:textAlignment="center"
                    android:textSize="@dimen/topBar_Title_Size"
                    android:textStyle="bold"
                    android:textColor="@android:color/white"/>
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="55dp"
                android:gravity="end">
                <Button
                    android:id="@+id/search"
                    android:layout_marginTop="13dp"
                    android:layout_marginRight="@dimen/topBar_Icon_Margin"
                    android:layout_width="@dimen/topBar_TextButton_Width"
                    android:layout_height = "@dimen/topBar_TextButton_Height"
                    android:text="수정"
                    android:textAlignment="textEnd"
                    android:textSize="@dimen/topBar_Title_Size"
                    android:textColor="@android:color/white"
                    android:background="@android:color/transparent"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    //top calender
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:layout_marginTop="55dp"
            android:orientation="horizontal">

            <Button
                android:background="@color/colorGray1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.3"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_sun_1"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_mon_1"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_tue_1"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_wed_1"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_thu_1"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_fri_1"/>
            <Button
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/cal_sat_1"/>
        </LinearLayout>
    </LinearLayout>
    //END OF CALENDAR TOP

    REST OF THE CALENDAR
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="59dp"
        android:layout_height="wrap_content">

        YEAR AND MONTH
        <RelativeLayout
            android:layout_weight="1.3"
            android:layout_width="0dp"
            android:layout_height="65dp"
            android:orientation="vertical"
            android:background="@color/colorGray1">
            YEAR
            <LinearLayout
                android:layout_marginTop="1dp"
                android:id="@+id/cal_yearView"
                android:layout_width="match_parent"
                android:layout_height="18dp">
                <Button
                    android:paddingTop="1dp"
                    android:layout_gravity="end"
                    android:background="@color/colorGray1"
                    android:text="1000"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/colorBlueGray1"
                    android:textSize="16sp"
                    android:id="@+id/cal_year"/>
            </LinearLayout>
            MONTH
            <LinearLayout
                android:layout_below="@id/cal_yearView"
                android:layout_width="match_parent"
                android:layout_height="32dp">
                <Button
                    android:layout_gravity="top"
                    android:background="@color/colorGray1"
                    android:text="00"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/colorCyanMain"
                    android:textSize="32sp"
                    android:textStyle="bold"
                    android:id="@+id/cal_month"/>
            </LinearLayout>
        </RelativeLayout>


        NAME AND DATE
        <LinearLayout
            android:layout_weight="7"
            android:layout_width="0dp"
            android:layout_height="65dp"
            android:orientation="vertical"
            android:background="@color/white">
            NAME
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:orientation="horizontal"
                android:background="@android:color/white"
                android:gravity="bottom">
                <Button
                    android:background="@android:color/white"
                    android:text="일"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@color/colorMagenta"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_sun_2"/>
                <Button
                    android:background="@android:color/white"
                    android:text="월"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@color/colorBlueGray1"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_mon_2"/>
                <Button
                    android:background="@android:color/white"
                    android:text="화"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@color/colorBlueGray1"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_tue_2"/>
                <Button
                    android:background="@android:color/white"
                    android:text="수"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@color/colorBlueGray1"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_wed_2"/>
                <Button
                    android:background="@android:color/white"
                    android:text="목"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@color/colorBlueGray1"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_thu_2"/>
                <Button
                    android:background="@android:color/white"
                    android:text="금"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@color/colorBlueGray1"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_fri_2"/>
                <Button
                    android:background="@android:color/white"
                    android:text="토"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="@dimen/calendar_Date_Name_Size"
                    android:id="@+id/cal_sat_2" />
            </LinearLayout>

            DATE
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:orientation="horizontal"
                android:background="@drawable/shape_bgwhite_bottomline1"
                android:paddingBottom="4dp">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@color/colorMagenta"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_sun_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@color/colorGray3"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_mon_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@color/colorGray3"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_tue_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@color/colorGray3"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_wed_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@color/colorGray3"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_thu_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@color/colorGray3"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_fri_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <Button
                        android:background="@android:color/transparent"
                        android:text="4"
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:textColor="@android:color/holo_blue_light"
                        android:textSize="@dimen/calendar_Date_Number_Size"
                        android:textStyle="bold"
                        android:id="@+id/cal_sat_3"
                        android:onClick="onClick_calendar"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="124dp">

    //room name view
    <LinearLayout
        android:id="@+id/roomnameview"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/shape_bgwhite_bottomline1"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/room_name_tv"
            android:text="방이름"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@color/colorCyanMain"/>
    </LinearLayout>

    //count bar
    <LinearLayout
        android:id="@+id/countbarview"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_below="@id/roomnameview"
        android:orientation="vertical"
        android:background="@drawable/shape_gray_sideline1"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="20dp"
            android:gravity="center"
            android:text="전체"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:textColor="@color/colorCyanMain"
                android:text="현원"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="외박"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="입원"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="기타"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:id="@+id/total_count_tv"
                android:text="전체"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:textColor="@color/colorCyanMain"
                android:id="@+id/normal_count_tv"
                android:text="현원"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:id="@+id/out_count_tv"
                android:text="외박"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:id="@+id/hospital_count_tv"
                android:text="입원"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="20dp"
                android:gravity="center"
                android:id="@+id/etc_count_tv"
                android:text="기타"/>
        </LinearLayout>
    </LinearLayout>

    //write mode selector
    <LinearLayout
        android:id="@+id/writemodeselector"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/countbarview"
        android:background="@drawable/shape_bgwhite_bottomline1">
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center">
            <Button
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:layout_gravity="center_vertical"
                android:text="전달사항"
                android:textSize="16sp"
                android:id="@+id/btn01"
                android:onClick="onClick_01btn"
                android:textColor="@color/colorCyanMain"
                android:background="@drawable/shape_radius_rect_selected_blue"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center">
            <Button
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:layout_gravity="center_vertical"
                android:text="식사량"
                android:textSize="16sp"
                android:id="@+id/btn02"
                android:onClick="onClick_02btn"
                android:textColor="@color/colorBlueGray1"
                android:background="@android:color/transparent"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center">
            <Button
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:layout_gravity="center_vertical"
                android:text="활동내역"
                android:textSize="16sp"
                android:id="@+id/btn03"
                android:onClick="onClick_03btn"
                android:textColor="@color/colorBlueGray1"
                android:background="@android:color/transparent"/>
        </LinearLayout>
    </LinearLayout>

    //tabs 1, 2, 3
    <LinearLayout
        android:layout_below="@id/writemodeselector"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/shape_bgwhite_bottomline1">

        //third tab linear layout
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/third_tab_linearlayout"
                android:visibility="gone"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="80dp"
                    android:orientation="horizontal"
                    android:background="@drawable/shape_bgwhite_bottomline1">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="오전"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorBlueDark1"
                        android:gravity="center"/>
                    <TextView
                        android:layout_marginRight="10dp"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/morning_program_tv"
                        android:text="야외활동"
                        android:textSize="18sp"
                        android:textColor="@color/colorBlueGray1"
                        android:gravity="center"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="80dp"
                    android:orientation="horizontal"
                    android:background="@drawable/shape_bgwhite_bottomline1">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="오후"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorBlueDark1"
                        android:gravity="center"/>
                    <TextView
                        android:layout_marginRight="10dp"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/afternoon_program_tv"
                        android:text="야외활동"
                        android:textSize="18sp"
                        android:textColor="@color/colorBlueGray1"
                        android:gravity="center"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="80dp"
                    android:orientation="horizontal"
                    android:background="@drawable/shape_bgwhite_bottomline1">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="저녁"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorBlueDark1"
                        android:gravity="center"/>
                    <TextView
                        android:layout_marginRight="10dp"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/night_program_tv"
                        android:text="야외활동"
                        android:textSize="18sp"
                        android:textColor="@color/colorBlueGray1"
                        android:gravity="center"/>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>

        //first tab article container
        <ListView
            android:id="@+id/firsttab_listview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

        //second tab image grid view
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_bgwhite_bottomline1"
            android:paddingLeft="8dp"
            android:layout_centerInParent="true">
            <GridView
                android:id="@+id/secondtab_grid_view"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:columnWidth="56dp"
                android:numColumns="auto_fit"
                android:verticalSpacing="5dp"
                android:horizontalSpacing="5dp"
                android:stretchMode="columnWidth"
                android:gravity="center"
                android:drawSelectorOnTop="true"
                android:visibility="gone"/>
        </LinearLayout>

        //second tab bottom meal imageview
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="15dp"
            android:background="@color/white">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:id="@+id/secondtab_meal_linearview"
                android:orientation="horizontal"
                android:visibility="gone">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="아침"
                        android:textStyle="bold"
                        android:textSize="18sp"
                        android:textColor="@color/colorBlueGray1"
                        android:textAlignment="center"/>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id ="@+id/meal_iv_1"
                        android:src="@drawable/intake_none"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="점심"
                        android:textStyle="bold"
                        android:textSize="18sp"
                        android:textColor="@color/colorBlueGray1"
                        android:textAlignment="center"/>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id ="@+id/meal_iv_2"
                        android:src="@drawable/intake_none"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="저녁"
                        android:textStyle="bold"
                        android:textSize="18sp"
                        android:textColor="@color/colorBlueGray1"
                        android:textAlignment="center"/>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id ="@+id/meal_iv_3"
                        android:src="@drawable/intake_none"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>



</RelativeLayout>

    //room select
    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        >
        <Button
